{
  "name": "starter",
  "version": "1.0.0",
  "description": "Simple webpack setup",
  "main": "src/index.js",
  "repository": "https://github.com/CoreyMartin/webpack-starter.git",
  "author": "Corey <coreyn.martin@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@coreymartin/eslint-config": "github:CoreyMartin/eslint-config#master",
    "babel-loader": "^8.0.0",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.3.1"
  },
  "scripts": {
    "build": "rimraf ./build && webpack --config webpack.prod.js",
    "serve": "webpack-dev-server --config webpack.dev.js",
    "lint": "eslint config.js webpack.prod.js webpack.dev.js app --color",
    "lint-commit": "lint-staged"
  },
  "pre-commit": {
    "run": [
      "lint-commit"
    ],
    "colors": true
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --quiet"
      ]
    },
    "ignore": [
      "build/**"
    ]
  }
}
